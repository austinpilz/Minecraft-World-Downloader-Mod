{
  "schemaVersion": 1,
  "id": "addon-template",
  "version": "${version}",
  "name": "World Archiver",
  "description": "A Meteor Client addon that handles flying navigation to assist in world download and archiving.",
  "authors": [
    "PilzBros"
  ],
  "contact": {
    "repo": "https://github.com/austinpilz/Minecraft-World-Downloader-Mod"
  },
  "icon": "assets/template/icon.png",
  "environment": "client",
  "entrypoints": {
    "meteor": [
      "com.pilzbros.archiver.ArchiverAddon"
    ]
  },
  "mixins": [
    "addon-template.mixins.json"
  ],
  "custom": {
    "meteor-client:color": "225,25,25"
  },
  "depends": {
    "java": ">=21",
    "minecraft": ["${mc_version}"],
    "meteor-client": "*"
  }
}
